{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "defaultSeverity": "warning",
  "rules": {
    "interface-name": false,
    "curly": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "array-type": false,
    "max-classes-per-file": false,
    "member-access": false,
    "jsx-no-lambda": false,
    "no-console": false
  }
}
